<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>createCharge</servlet-name>
		<servlet-class>com.kpoda.ping.CreateChargeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createCharge</servlet-name>
		<url-pattern>/createCharge</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>chargeSuccess</servlet-name>
		<servlet-class>com.kpoda.ping.ChargeSuccessServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>chargeSuccess</servlet-name>
		<url-pattern>/chargeSuccess</url-pattern>
	</servlet-mapping>
	
</web-app>
